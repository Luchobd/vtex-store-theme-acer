  // Disclosure Products
  "disclosure-layout#products": {
    "title": "Disclosure layout products mobile",
    "children": ["disclosure-trigger#products", "disclosure-content#products"],
    "props": {
      "blickClass": "footer__disclosure--products"
    }
  },

  "disclosure-trigger#products": {
    "title": "Disclosure trigger products mobile",
    "children": ["rich-text#products", "disclosure-state-indicator"],
    "props": {
      "blockClass": "footer__trigger--products"
    }
  },

  "rich-text#products": {
    "props": {
      "text": "Productos",
      "blockClass": "footer__title--products"
    }
  },

  "disclosure-state-indicator": {
    "title": "disclosure__icon--down-up",
    "props": {
      "Show": "icon-caret#down",
      "Hide": "icon-caret#up",
      "blockClass": "footer__disclosure--indicator"
    }
  },

  "icon-caret#down": {
    "title": "disclosure__icon--down",
    "props": {
      "orientation": "down",
      "activeClassName": "green"
    }
  },
  "icon-caret#up": {
    "title": "disclosure__icon--up",
    "props": {
      "orientation": "up",
      "activeClassName": "green"
    }
  },

  "disclosure-content#products": {
    "title": "Disclosure content products",
