{
  "disclosure-layout-group#footer__group--mobile": {
    "title": "Disclosure Container mobile",
    "children": [
      "disclosure-layout#products",
      "disclosure-layout#support",
      "disclosure-layout#resources",
      "disclosure-layout#about"
    ],
    "props": {
      "blockClass": "footer__disclosure--container"
    }
