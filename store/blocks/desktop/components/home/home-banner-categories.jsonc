  "flex-layout.row#banner-categories__phones--container-overlay": {
    "title": "Phones Container Overlay",
    "props": {
      "blockClass": "banner-categories__container-overlay"
    },
    "children": [
      "link#phones__container-overlay--title",
      "link#phones__container-overlay--content",
      "link#phones__container-overlay--button"
