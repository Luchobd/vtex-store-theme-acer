  "rich-text#banner-categories__phones--title": {
    "title": "Category Phones Title",
    "props": {
      "text": "Ofertas para Empresas",
      "font": "t-heading-5",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "banner-categories__phones--title"
