{
  // Productos select
  "menu-item#more": {
    "title": "header more",
    "props": {
      "id": "menu-item-custom-sale",
      "type": "custom",
      "highlight": false,
      "blockClass": "header__category--select-hover",
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "Productos",
        "text": "Productos",
        "iconPosition": "right"
      },
      "iconProps": {
        "id": "nav-caret--down",
        "size": 12,
        "viewBox": "0 0 16 16",
        "activeClassName": "green",
        "mutedClassName": "c-action-primary"
      }
    },
    "blocks": ["vtex.menu@2.x:submenu#more"]
  },

  // Soluciones select
  "menu-item#more2": {
    "title": "header more2",
    "props": {
      "id": "menu-item-custom-sale",
