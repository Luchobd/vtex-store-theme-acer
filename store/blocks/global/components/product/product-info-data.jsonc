{
  "flex-layout.row#product__info": {
    "title": "overview pdp",
    "children": [
      "flex-layout.col#product__info--image",
      "flex-layout.col#product__info--data"
    ],
    "props": {
      "blockClass": "product__info"
    }
  },
  "flex-layout.col#product__info--image": {
    "title": "Image product",
    "children": ["product-images#product__info--image"],
    "props": {
      "blockClass": "product__info--image"
    }
  },
  "product-images#product__info--image": {
    "title": "Images",
    "props": {
      "zoomMode": "open-modal",
      "blockClass": "product__info--image"
    }
  },

  "flex-layout.col#product__info--data": {
    "title": "Data product",
    "children": [
      // "share",
      "product-name",
      "product-brand",
      "image#product__info--image-widnows11",
      // "product-rating-inline",
      "product-description",
      "product-price"
      // "product-quantity",
      // "product-description",
      // "shipping-simulator",
      // "availability-subscriber"
    ],
    "props": {
      "blockClass": "product__info--data"
    }
  },
  "image#product__info--image-widnows11": {
    "title": "product info image widnows11",
    "props": {
      "src": "assets/img/windows11-update.webp",
      "maxHeight": 24,
      "blockClass": "product__info--image-widnows11"
    }
  }
}
